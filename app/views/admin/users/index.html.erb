<h1>Admin Users</h1>
<table class="table table-striped table-hover">
  <thead>
  <td>ID</td>
  <td>EMAIL</td>
  <td>ROLE</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
    </tr>
  <% end %>
</table>