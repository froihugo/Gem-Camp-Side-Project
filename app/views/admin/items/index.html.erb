<h3 class="text-center">Items</h3>
<table class="table table-striped table-hover">
      <td>Image</td>
      <td>Name</td>
      <td>Quantity</td>
      <td>Minimum Bets</td>
      <td>State</td>
      <td>Batch Count</td>
      <td>Online At</td>
      <td>Offline At</td>
      <td>Start At</td>
      <td>Status</td>
      <td>categories</td>
      <td colspan="2">Action</td>
    </tr>
  </th>
  <% @items.each do |item| %>
    <tbody>
    <tr>
      <td><%= image_tag item.image.url || default_iamge %></td>
      <td><%= item.name %></td>
      <td><%= item.quantity %></td>
      <td><%= item.minimum_bets %></td>
      <td><%= item.state %></td>
      <td><%= item.batch_count %></td>
      <td><%= item.online_at %></td>
      <td><%= item.offline_at %></td>
      <td><%= item.start_at %></td>
      <td><%= item.status %></td>
      <td><%= item.categories.pluck(:name).join(', ') %></td>
      <td><%= link_to 'Show', admin_item_path(item), class: 'btn btn-outline-danger me-2' %></td>
    </tr>
    </tbody>
  <% end %>
</table>
<%= link_to '+Add New Item', new_admin_item_path, class: 'btn btn-success' %>