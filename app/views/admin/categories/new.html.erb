<%= render 'layouts/admins/error_message', resource: @category %>
    <h3 class="text-center">Add Category</h3>
    <%= form_with model: @category, url: admins_categories_path, data: { turbo: false } do |form| %>
        <%= form.label :name, class: 'form-label' %><br>
        <%= form.text_field :name, class: 'form-control' %><br>
        <%= form.submit class: 'btn btn-primary mx-1' %><%= link_to "Back", new_admins_item_path, class: 'btn btn-primary' %>
    <% end %>